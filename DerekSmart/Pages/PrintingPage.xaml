<Page x:Class="DerekSmart.Pages.PrintingPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DerekSmart.Pages"
	  xmlns:cc="clr-namespace:DerekSmart.CustomControls"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PrintingPage">

    <StackPanel>
		<TextBlock Text="Select Printer" FontSize="32" Foreground="{DynamicResource TextFillColorPrimaryBrush}"/>
		<ComboBox Height="80">
			<cc:SmallPrinterInfo/>
			<cc:SmallPrinterInfo/>
			<cc:SmallPrinterInfo/>
		</ComboBox>
	</StackPanel>
</Page>
